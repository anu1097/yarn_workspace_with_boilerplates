{
	"defaultSeverity": "error",
	"extends": [
			"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"indent":[true, "tabs", 2],
		"interface-name": false,	
		"member-access": false,	
		"no-trailing-whitespace":true,
		"no-console": true,	
		"no-submodule-imports": false,
		"object-literal-sort-keys": false
	},
	"rulesDirectory": []
}